/**
 * @param {number[]} nums
 * @return {number}
 */
var longestBalanced = function (nums) {
    let length = 0;
    for (let i = 0; i < nums.length - 1; i++) {
        let odd = new Set(), even = new Set();
        if (nums[i] % 2 == 0) {
            even.add(nums[i]);
        }
        else {
            odd.add(nums[i]);
        }
        for (let j = i + 1; j < nums.length; j++) {
            if (nums[j] % 2 == 0) {
                even.add(nums[j]);
            }
            else {
                odd.add(nums[j]);
            }
            if (even.size == odd.size) {
                length = Math.max(length, j - i + 1);
            }
        }
    }
    return length;
};
