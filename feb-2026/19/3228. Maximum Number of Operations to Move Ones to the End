/**
 * @param {string} s
 * @return {number}
 */
var maxOperations = function(s) {
    let i=s.length-1;
    let flag;
    let count=0,ans=0;
    while(i>=0){
        if(s[i]=='0'){
            flag=1;
            i--;
        }
        else{
            if(flag==1){
                ans+=count+1;
                i--;
                while(i>=0&&s[i]=='1'){
                    ans+=count+1;
                    i--;
                }
                count+=1;
            }
            i--;
        }
    }
    return ans;
};
